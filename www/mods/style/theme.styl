html, body, .pane, .view{
	background-color: #f5f2ec;
    /*font-family: "微软雅黑";*/
}
.h1,h2,h3,h4,h5,h6{
    /*font-family: "微软雅黑";*/
}
.bar-primary{
	background-color: rgba(66, 38, 89, .85);
  /*-webkit-backdrop-filter: blur(4px);*/
}
.bar-primary .title{
	color: #fff;
}
.bar-primary .button {
  border-color: transparent;
  background-color: transparent;
  color: #fff;
 }
 .bar-primary .back-text{
     font-size: 14px;
 }
  .bar-primary .button:hover {
    color: #fff;
    text-decoration: none; }
  .bar-primary .button.active, .bar-primary .button.activated {
    border-color: transparent;
    background-color: transparent;
    box-shadow: inset 0 1px 4px rgba(0, 0, 0, 0.1); }
  .bar-primary .button.button-clear {
    border-color: transparent;
    background: none;
    box-shadow: none;
    color: #fff;
    font-size: 17px; }
  .bar-primary .button.button-icon {
    border-color: transparent;
    background: none; }
/*.has-header{*/
.scroll-content{
	top: 0;
	padding-top: 44px;
}
.has-tabs{
    bottom: 0;
    padding-bottom: 49px;
}
.tabs-primary > .tabs, .tabs.tabs-primary{
    /*background-color: #e4dfd6;*/
    background-color: rgba(228, 223, 214, 0.95);
    background-image: linear-gradient(180deg, #B5A891, #B5A891 50%, transparent 50%);
    color: #5d446f;
    -webkit-backdrop-filter: blur(4px);
    padding-top: 0; 
}
.tabs-primary .tab-item{
    line-height: 1;
}
.tab-item{
    opacity: 1;
    padding-top: 5px;
}
.tab-item .icon{
    font-size: 28px;
    height: 28px;
    line-height: 1;
    -webkit-text-stroke-width: 0;
}
.tab-item .tab-title{
    display: block;
    margin-top: 1px;
    font-size: 10px;
}
.tab-item-active{
  color: #EC8600;
  transition: 0.1s linear color;
}
.tab-item.activated{
    background-color: #DAD3C9;
  box-shadow: 1px 1px 2px rgba(0, 0, 0, .1) inset;
}
.tab-item.activated *{
    transform: translate3d(1px, 1px, 0);
}
.placeholder{
  color: #bbb;
}